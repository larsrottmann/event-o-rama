<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:orientation="vertical"
	android:background="@color/generic_bg_color">
	<include
		layout="@layout/nav_bar"
		android:id="@+id/nav_bar_include"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_margin="0px"
		android:layout_weight="0" />


	<ListView
		android:id="@android:id/list"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_weight="1" android:background="@color/generic_bg_color"/>

	<LinearLayout
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:id="@+id/relativeLayout2"
		android:layout_gravity="bottom"
		android:layout_alignParentBottom="true"
		android:layout_weight="0">
		<Button
			android:layout_height="wrap_content"
			android:text="Refresh People"
			android:id="@+id/button4"
			android:layout_weight="1"
			android:layout_width="0dp"></Button>
		<Button
			android:layout_height="wrap_content"
			android:text="Refresh Activities"
			android:id="@+id/button3"
			android:layout_weight="1"
			android:layout_width="0dp"></Button>
		<Button
			android:text="Send Location"
			android:layout_height="wrap_content"
			android:id="@+id/button5"
			android:layout_weight="1"
			android:layout_width="0dp"></Button>
		<Button
			android:layout_height="wrap_content"
			android:text="What's app?"
			android:id="@+id/button1"
			android:layout_alignParentRight="true"
			android:layout_weight="1"
			android:layout_width="0dp"></Button>

	</LinearLayout>


</LinearLayout>
