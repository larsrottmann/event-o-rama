<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	android:background="@color/generic_bg_color">
	
	<include
	layout="@layout/nav_bar_simple"
	android:id="@+id/nav_bar_relative_layout_ref"
	android:layout_width="match_parent"
	android:layout_height="wrap_content" />
	<TextView
		android:text="What's app?"
		android:id="@+id/textView1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:textSize="24dip"
		android:textColor="@color/generic_text_color"></TextView>
	<EditText
		android:layout_height="wrap_content"
		android:layout_width="match_parent"
		android:id="@+id/editText1"
		android:inputType="textMultiLine">
		<requestFocus></requestFocus>
	</EditText>
	<Button
		android:text="Share!"
		android:id="@+id/sharebutton1"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"></Button>

</LinearLayout>
