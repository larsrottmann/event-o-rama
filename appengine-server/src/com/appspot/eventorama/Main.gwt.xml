<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='main'>
	<inherits name="com.google.gwt.user.User" />
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
	<inherits name="org.slim3.gwt.emul.S3Emulation" />
	<entry-point class="com.appspot.eventorama.client.Main"/>
	<source path="client"/>
	<source path="shared"/>

	<!-- Set this property to true to turn on UiBinder's new SafeHtmlTemplates 
		integration. The default value will be true in future releases of GWT -->
	<set-configuration-property name="UiBinder.useSafeHtmlTemplates"
		value="true" />
</module>
